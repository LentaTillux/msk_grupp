---
title: ПАРТНЕРАМ
---
extends layouts/_layout
block content
	//begin main
	+b.main
		//- page-info
		-pageInfo = {title: 'ПАРТНЕРАМ'}
		-breadcrumbsList = ['главная', 'ПАРТНЕРАМ']
		-activeIndex = 2
		include components/_page-info

	//-team
	-teamClass = '_partners'
	-teamTitle = 'Торговое/Стратегическое сотрудничество';
	-teamSubtitle = 'Контакты для вопросов сотрудничества по территории РФ';
	-teamLead = false;

	//begin team
	+b.SECTION.team(class= teamClass ? teamClass : '')
		+e.container.container
			+e.H3.title.title-md= teamTitle
			if teamSubtitle
				+e.P.sub-title= teamSubtitle
			+e.inner
				if teamLead !== false
					+e.lead
						+e.person-img
							+e.IMG(src='img/content/team-director.png' alt='img')
						+e.person-title Иван Ивановский
						+e.person-position Генеральный директор
						+e.P.lead-text Уважаемые заказчики Мы ради сотрудничеству с Вами. Мы всегда готовы помочь Вам ведь мы Роман Казаченко Иванов Иван Екатерина Волошановская работаем исключительно на результат.
						+e.P.lead-text С уважением.
						+icon('icon-quotes')
				+e.squad.js-partners-slider
					-person = 0
					while person < 2
						+e.person
							+e.person-img
								+e.IMG(src='img/content/team-man.png' alt='img')
							+e.person-title Иван Ивановский
							+e.person-position Технический директор
							+e.person-contacts
								+e.A.person-email(href='#!') roman@yandex.ru
								+e.A.person-phone(href='#!') +7 (846) 340-61-91
								+e.person-social
									+e.A.person-social-link(href='#!')
										+icon('icon-fb')

						+e.person
							+e.person-img
								+e.IMG(src='img/content/team-woman.png' alt='img')
							+e.person-title Екатерина Волошанова
							+e.person-position МАРКЕТОЛОГ
							+e.person-contacts
								+e.A.person-email(href='#!') volonashova@yandex.ru
								+e.A.person-phone(href='#!') +7 (846) 340-61-91
								+e.person-social
									+e.A.person-social-link(href='#!')
										+icon('icon-fb')
						-person++
	//end team

	//-contact-block
	-contactBlockClass = '_partners'
	-contactFormLgTitle = 'Партнерская программа'
	-contactFormSmTitle = 'Заполните специальную форму и в течение 24 часов наш эксперт по партнерской программе свяжется с Вами'

	//begin contact-block
	+b.SECTION.contact-block(class= contactBlockClass ? contactBlockClass : '')
		+e.container.container
			if contactFormLgTitle
				+b.H3.title-lg= contactFormLgTitle
			if contactFormSmTitle
				+b.H3.title-sm-white= contactFormSmTitle
			+e.FORM.form(action='#!')
				+e.form-row
					+e.form-field
						+e.INPUT.input.input._gray(type='text' placeholder='Имя*')
					+e.form-field
						+e.INPUT.input.input._gray(type='text' placeholder='Отчество*')
					+e.form-field
						+e.INPUT.input.input._gray(type='text' placeholder='Фамилия*')
				+e.form-row
					+e.form-field
						+e.INPUT.input.input._gray(type='text' placeholder='Телефон')
					+e.form-field
						+e.INPUT.input.input._gray(type='text' placeholder='E-mail')
					+e.form-field
						+e.INPUT.input.input._gray(type='text' placeholder='Название организации')
				+e.form-row
					+e.TEXTAREA.textarea.textarea._popup(placeholder='Текст вашего сообщения')

				+e.P.form-descr * Оставляя свои данные вы соглашаетесь с политикой конфедециальности
				+e.BUTTON.form-btn.btn._aqua(type='submit') оставить заявку
				+e.LABEL.form-btn.btn_file прикрепить файл
					+icon('icon-file')
					+b.INPUT(type='file' accept='')

		+b.particles#js-gray-particles
	//end contact-block